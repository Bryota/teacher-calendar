<div>
    <h1>ユーザー追加</h1>
    <%= form_with model: @user, local: true do |f| %>
        <%= render 'common/error' %>
        <div class="form-group d-flex pt-4">
            <%= f.label :name, '氏名'%>
            <%= f.text_field :name, class: 'form-control' %>
        </div>
        <div class="form-group d-flex pt-4">
            <%= f.label :email, 'Eメール'%>
            <%= f.text_field :email, class: 'form-control' %>
        </div>
        <%= f.submit '送信', class: 'btn btn__reserve' %>
    <% end %>
</div>